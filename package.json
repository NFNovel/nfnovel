{
  "name": "nfnovel-dapp",
  "version": "1.0.0",
  "description": "NFNovel MVP project",
  "scripts": {
    "install": "npm run client -- install && npm run solidity -- install",
    "build": "npm run solidity:build && npm run client:build",
    "predev": "npm run solidity:deploy",
    "dev": "npm run solidity:build && npm run client:dev",
    "client": "npm --prefix ./client",
    "client:": "npm run client -- run",
    "client:build": "npm run client: -- build",
    "client:dev": "npm run client: -- dev",
    "solidity": "npm --prefix ./evm",
    "solidity:": "npm run solidity -- run",
    "solidity:build": "npm run solidity: -- build",
    "solidity:deploy": "npm run solidity: -- deploy"
  },
  "keywords": [],
